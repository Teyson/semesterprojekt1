<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sp1.guisemesterprojekt1.StartMenuController">
   <children>
      <StackPane prefHeight="600.0" prefWidth="800.0">
         <children>
         
            <ImageView fx:id="backgroundImage" fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="startGameBtn" fitHeight="75.0" fitWidth="220.0" onMouseClicked="#startGameOnAction" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="danceView" fitHeight="158.0" fitWidth="114.0" pickOnBounds="true" preserveRatio="true">
               <StackPane.margin>
                  <Insets top="350.0" />
               </StackPane.margin>
            </ImageView>
         </children>
      </StackPane>
   </children>
               
</AnchorPane>
