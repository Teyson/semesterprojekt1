<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sp1.guisemesterprojekt1.StartMenuController">
   <children>
      <StackPane prefHeight="600.0" prefWidth="800.0">
         <children>
            <ImageView fx:id="backgroundImage" fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="startGameBtn" defaultButton="true" mnemonicParsing="false" onAction="#startGameOnAction" prefHeight="74.0" prefWidth="222.0" text="Start Game" textAlignment="CENTER">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
         </children>
      </StackPane>
   </children>
</AnchorPane>
