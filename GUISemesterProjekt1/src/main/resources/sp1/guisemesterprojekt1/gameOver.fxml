<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sp1.guisemesterprojekt1.GameOverController">
    <children>
        <StackPane onMouseClicked="#handleRestartEvent" prefHeight="600.0" prefWidth="800.0">
            <children>
                <ImageView fx:id="backgroundImage" fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" />
                <ImageView fx:id="backgroundGIF" fitHeight="250.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
            </children>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </StackPane>
    </children>
</AnchorPane>
